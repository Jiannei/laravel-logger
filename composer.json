{
    "name": "jiannei\/laravel-logger",
    "description": "Laravel logger.",
    "license": "MIT",
    "authors": [
        {
            "name": "jiannei",
            "email": "longjian.huang@foxmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "jenssegers/mongodb": "3.8.x",
        "jiannei/laravel-enum": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.4",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Jiannei\\Logger\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiannei\\Logger\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiannei\\Logger\\Laravel\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
